objectClasses: ( fhirUser-OID NAME 'fhirUser' DESC 'User object pre-HL7 pre-FHIR definitions.' SUP top STRUCTURAL MUST uid MAY ( cn $ description $ displayName $ employeeNumber $ employeeType $ fhirAccountNumber $ fhirAccountVerified $ fhirCommOpts $ fhirContentOpts $ fhirDeviceID $ fhirEntitlement $ fhirLastLoginType $ fhirOptionalDN $ fhirOptionalJSON $ fhirOptionalString $ fhirOrganizationDN $ fhirOrganizationID $ fhirPersona $ fhirRegion $ fhirRole $ fhirTermsOfService $ fhirTimeZone $ fhirTopicPreference $ fhirType $ fhirVisit $ givenName $ mail $ o $ seeAlso $ sn $ telephoneNumber $ userPassword ) X-ORIGIN 'Ping Identity RSA' )
objectClasses: ( fhirPerson-OID NAME 'fhirPerson' DESC 'Person object per FHIR definitions. It is a child of fhirUser.' SUP fhirUser STRUCTURAL MAY ( fhirActive $ fhirAddress $ fhirBirthDate $ fhirGender $ fhirIdentifier $ fhirLink $ fhirManagingOrg $ fhirName $ fhirTelecom $ photo ) X-ORIGIN 'Ping Identity RSA' )
objectClasses: ( fhirOrganization-OID NAME 'fhirOrganization' DESC 'Organization object per FHIR definitions. It is a child of Organization.' SUP organization STRUCTURAL MAY ( displayName $ fhirAccountNumber $ fhirAccountVerified $ fhirActive $ fhirContact $ fhirEndpoint $ fhirEntitlement $ fhirIdentifier $ fhirName $ fhirOptionalDN $ fhirOptionalJSON $ fhirOptionalString $ fhirOrganizationDN $ fhirPartOf $ fhirRegion $ fhirRole $ fhirType ) X-ORIGIN 'Ping Identity RSA' )
objectClasses: ( fhirPractitioner-OID NAME 'fhirPractitioner' DESC 'Practitioner object per FHIR definitions. A Practitioner may also be a Patient.' AUXILIARY MAY ( fhirCommunication $ fhirPractitionerRole $ fhirQualification ) X-ORIGIN 'Ping Identity RSA' )
objectClasses: ( fhirPatient-OID NAME 'fhirPatient' DESC 'Patient object per FHIR definitions. A Patient may also be a Practitioner.' AUXILIARY MAY ( fhirAddress $ fhirAnimal $ fhirBirthDate $ fhirCommunication $ fhirContact $ fhirDeceased $ fhirDeceasedDate $ fhirFlag $ fhirGender $ fhirGeneralPractitioner $ fhirIdentifier $ fhirLink $ fhirManagingOrg $ fhirMaritalStatus $ fhirMultipleBirth $ fhirMultipleBirthInteger $ fhirName $ fhirRelatedPerson $ fhirTelecom $ photo ) X-ORIGIN 'Ping Identity RSA' )

