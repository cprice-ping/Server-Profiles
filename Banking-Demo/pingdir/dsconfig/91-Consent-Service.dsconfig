dsconfig set-consent-service-prop --set enabled:true --set base-dn:ou=Consents,${USER_BASE_DN} --set bind-dn:cn=administrator --set "consent-record-identity-mapper:entryUUIDMatch" --set "consent-record-identity-mapper:Exact Match" --set service-account-dn:cn=administrator --set unprivileged-consent-scope:pd:consents:unpriv --set privileged-consent-scope:pd:consents:priv

dsconfig set-http-servlet-extension-prop --extension-name Consent --set "access-token-validator:Mock ATV"

dsconfig create-consent-definition --definition-name EMR-Access --set "display-name:Banking Terms & Conditions" --set "description:This definition is use to ask and collect consent for accepting the Terms and Conditions of the Banking app(s)"

dsconfig create-consent-definition-localization --definition-name EMR-Access --localization-name en --set version:1.0 --set "title-text:Access to Medical Record" --set 'data-text:You agree to all the Terms and Conditions laid out in this text' --set 'purpose-text:Acceptance of all Terms and Conditions'
